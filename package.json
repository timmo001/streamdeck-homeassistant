{
  "name": "streamdeck-homeassistant",
  "homepage": "",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "autoprefixer": "10.2.1",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "10.0.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "4.1.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.22.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "fs-extra": "9.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.4.1",
    "react-streamdeck": "^1.2.2",
    "resolve": "1.19.0",
    "style-loader": "2.0.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "4.1.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.5",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "3.5.0"
  },
  "scripts": {
    "start": "echo \"Use yarn|npm dev or dev:watch to compile this plugin\"",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "export:base": "node scripts/export.js",
    "export": "npm-run-all -s build export:base",
    "dev:base": "node scripts/dev.js",
    "dev": "cross-env NODE_ENV=development yarn build && yarn dev:base",
    "dev:watch": "nodemon --ext ts,tsx,js,css,html --watch public --watch src --exec \"yarn dev\""
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(ts|tsx|js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(ts|tsx|js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/node": "14.14.21",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "cross-env": "7.0.3",
    "eslint-plugin-react-hooks": "4.2.0",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "typescript": "4.1.3",
    "unzipper": "0.10.11"
  }
}
